programa {
  funcao inicio() {
    inteiro angulo1, angulo2, angulo3

        escreva("Digite o ângulo 1: \n")
        leia(angulo1)

        escreva("Digite o ângulo 2: \n")
        leia(angulo2)

        angulo3 = 180 - (angulo1 + angulo2)

        escreva("O terceiro ângulo do triângulo é: ", angulo3, " graus\n")
  }
}
